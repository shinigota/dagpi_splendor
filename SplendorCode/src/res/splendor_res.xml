<?xml version="1.0" encoding="UTF-8"?>

<!-- Elements of the game Splendor. -->
<game_elements>

	<!-- Single parameters. -->
	<single_parameters>
		<game_name>Splendor</game_name> <!-- Name of the game. -->
		<number_levels_cards>3</number_levels_cards> <!-- Number of levels of the development cards. -->
		<number_max_reserved_cards>3</number_max_reserved_cards> <!-- Number maximum of reserved cards in the hand of any player.-->
		<number_prestige_points_noble_tiles>3</number_prestige_points_noble_tiles> <!-- Number of prestige points of noble tiles.-->
		<number_prestige_points_end_game>15</number_prestige_points_end_game> <!-- Number of prestige points a player must reach to end the game.-->
	</single_parameters>

	<!-- Gem and gold tokens. -->
	<tokens>
		<!-- Gem tokens. -->
		<token>
			<name>Emerald</name>
			<color>green</color>
		</token>
		<token>
			<name>Diamond</name>
			<color>white</color>
		</token>
		<token>
			<name>Sapphire</name>
			<color>blue</color>
		</token>
		<token>
			<name>Onyx</name>
			<color>black</color>
		</token>
		<token>
			<name>Ruby</name>
			<color>red</color>
		</token>
		<!-- Gold (joker) tokens. -->
		<token>
			<name>Gold</name>
			<color>yellow</color>
		</token>
	</tokens>

	<!-- Game setup. -->
	<game_setup>
		<number_cards_to_reveal>4</number_cards_to_reveal> <!-- Number of development cards to reveal from each level. -->
		<number_noble_tiles_more_number_players>1</number_noble_tiles_more_number_players> <!-- Number of noble tiles to reveal = as many of them as there are players plus one; example: 5 tiles for a 4 player game. -->
		<number_gem_tokens_in_game_according_number_players> <!-- Number of each gem token during the game according to the number of players. -->
			<number_gem_tokens_in_game_2_players>4</number_gem_tokens_in_game_2_players>
			<number_gem_tokens_in_game_3_players>5</number_gem_tokens_in_game_3_players>
			<number_gem_tokens_in_game_4_players>7</number_gem_tokens_in_game_4_players>
		</number_gem_tokens_in_game_according_number_players>
		<number_gold_tokens_in_game>5</number_gold_tokens_in_game> <!-- Number during the game of gold tokens. -->
	</game_setup>

	<!-- Turn parameters. -->
	<turn_parameters>
		<number_gem_tokens_different_colors>3</number_gem_tokens_different_colors> <!-- Number of gem tokens of different colors by turn. -->
		<number_gem_tokens_same_color>2</number_gem_tokens_same_color> <!-- Number of gem tokens of the same color by turn. -->
		<number_min_gem_tokens_stack_if_same_color>4</number_min_gem_tokens_stack_if_same_color> <!-- Number minimum of gem tokens in its stack when the player takes gem tokens of the same color during this turn. -->
		<number_max_gold_tokens_if_card_reserved>1</number_max_gold_tokens_if_card_reserved> <!-- Number maximum of gold tokens to take if a development card is reserved during this turn. -->
		<number_max_tokens_end_turn>10</number_max_tokens_end_turn> <!-- Number maximum of tokens at the end of the turn. -->
		<number_max_noble_tile_turn>1</number_max_noble_tile_turn> <!-- Number maximum of noble tile for a player per turn. -->
	</turn_parameters>

